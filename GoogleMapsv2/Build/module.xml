		<jar scope="DC">googlemapsv2-client.jar.pack.gz</jar>


		<!-- Tell the Gateway and/or Designer where to find the hooks -->
		<hook scope="D">com.mycompany.mymodule.designer.DesignerHook</hook>

</module>

